{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.{ts,js}": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/tiktok-post",
      "schedule": "*/5 * * * *"
    }
  ]
}
